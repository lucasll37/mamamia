    "@mui/icons-material": "^5.15.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "axios": "^1.6.2",
    "recharts": "^2.10.3",
    "react-dropzone": "^14.2.3",
    "react-query": "^3.39.3",
    "zustand": "^4.4.7",
    "date-fns": "^3.0.0",
